        </Field>
        <Field Name="ShippingStateOrProvince">
          <DataField>ShippingStateOrProvince</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ShippingZip5">
          <DataField>ShippingZip5</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
