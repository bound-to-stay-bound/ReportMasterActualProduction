    <DataSet Name="BTSB_Dataset_PreviewDetailHeader">
      <Query>
        <DataSourceName>BTSB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CustomerNo">
            <Value>=Parameters!CustomerNo.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OrderNo">
            <Value>=Parameters!OrderNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT top 1 h.ShippingAttn1, h.ShippingAttn2, h.ShippingDestinationName, h.ShippingStreet1, h.ShippingCity, h.ShippingStateOrProvince, h.ShippingZip5
FROM History.dbo.Headers h 
INNER JOIN History.dbo.Titles t on h.CustomerNo = t.CustomerNo and h.OrderNo = t.OrderNo and h.InvoiceNo = t.InvoiceNo 
INNER JOIN Orders.dbo.Addresses a on h.CustomerNo = a.CustomerNo 
INNER JOIN BTSB.dbo.TMASTER tm on t.TitleNo = tm.titleno
WHERE OrderType = 'PR' AND (SELECT COUNT(*) FROM Orders.dbo.Notes WHERE NoteType = 'O' AND TypeID = h.CustomerNo + CAST(h.OrderNo as VARCHAR) + CAST(h.InvoiceNo as VARCHAR) AND SubType = 'AM') &lt;= 0 
	AND h.CustomerNo = @CustomerNo AND (h.OrderNo = @OrderNo OR ISNULL(@OrderNo, '') = '')</CommandText>
      </Query>
      <Fields>
        <Field Name="ShippingAttn1">
          <DataField>ShippingAttn1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ShippingAttn2">
          <DataField>ShippingAttn2</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ShippingDestinationName">
          <DataField>ShippingDestinationName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ShippingStreet1">
          <DataField>ShippingStreet1</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ShippingCity">
          <DataField>ShippingCity</DataField>
