    <DataSet Name="BTSB_Dataset_HoldSummaryTotals">
      <Query>
        <DataSourceName>BTSB</DataSourceName>
        <CommandText>SELECT COUNT(*) as TotalOrders, SUM(QtyTitlesAvailable) as TotalAvailable
FROM Orders.dbo.Headers
WHERE OnHold = 1</CommandText>
