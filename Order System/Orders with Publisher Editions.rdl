  <DataSets>
    <DataSet Name="BTSB_Dataset_TXNonLogo">
      <Query>
        <DataSourceName>BTSB</DataSourceName>
        <CommandText>SELECT t.TitleNo, tm.ShortAuthorName AS Author, RTRIM(tm.ShortMarketingTitle) AS Title, t.CustomerNo, t.OrderNo, t.RequestedQty, t.AvailableQty,CASE h.AllocateBooks WHEN 1 THEN 'Order' ELSE 'Quote' END AS [Order Quote],CASE t.IsPicked WHEN 1 THEN 'Picking Copy' ELSE '' END AS Picked, t.DateAdded
FROM Orders.dbo.Titles t
JOIN BTSB.dbo.FULLTMASTER tm ON t.TitleNo = tm.titleno
JOIN Orders.dbo.Headers h ON t.CustomerNo = h.CustomerNo AND t.OrderNo = h.OrderNo
JOIN Orders.dbo.Customers c ON  t.CustomerNo = c.customerNo
JOIN Orders.dbo.Addresses a ON c.MainAddress = a.AddressID
WHERE tm.pub_bind in ('Y', 'P')
ORDER BY CustomerNo, OrderNo, t.Titleno</CommandText>
      </Query>
      <Fields>
        <Field Name="TitleNo">
          <DataField>TitleNo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Author">
          <DataField>Author</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
